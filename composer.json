{
    "name": "FreakXoHBingo",
    "keywords": ["bingo", "freakshow"],
    "homepage": "http://freakxohbingo.de/",
    "license": "MIT",
    "authors": [
        {
            "name": "Timo KÃ¶nnecke",
            "email": "koennecke@mosushi.com",
            "homepage": "http://mosushi.com"
        },
        {
            "name": "Adam Ibrom",
            "email": "adam@net-i.de",
            "homepage": "http://net-i.de"
        }
    ],
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phpunit/phpunit": "5.4.x-dev",
        "phpunit/phpunit-mock-objects": "3.1.x-dev",

        "doctrine/orm": "2.6.x-dev",
        "doctrine/doctrine-bundle": "1.6.x-dev",

        "symfony/symfony": "2.8.x-dev",
        "symfony/assetic-bundle": "2.7.x-dev",
        "symfony/swiftmailer-bundle": "2.3.x-dev",
        "symfony/monolog-bundle": "2.8.x-dev",

        "sensio/distribution-bundle": "4.0.x-dev",
        "sensio/framework-extra-bundle": "3.0.x-dev",

        "incenteev/composer-parameter-handler": "2.1.x-dev",

        "propel/propel1": "1.6.x-dev",
        "propel/propel-bundle": "1.5.x-dev",

        "willdurand/propel-typehintable-behavior": "dev-master",

        "friendsofsymfony/rest-bundle": "dev-master",
        "jms/serializer-bundle": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "3.0.x-dev",
        "symfony/phpunit-bridge": "3.1.x-dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "app/console propel:model:build"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "rm -rf src/*/Propel/map/**",
            "rm -rf src/*/Propel/om/**",
            "app/console propel:model:build"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
